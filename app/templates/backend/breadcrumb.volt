<ol class="breadcrumb">{% set _breadcrumb = _toolbarHelpers.getBreadcrumb() %}{% if _breadcrumb is defined %}{% for index, bread in _breadcrumb %}<li>{% if bread['active'] %}{% if index == 0 %}<a href="#"><i class="fa fa-dashboard"></i> {{ __(bread['title']) }}</a>{% else %}<a href="#">{{ __(bread['title']) }}</a>{% endif %}{% else %}{{ __(bread['title']) }}{% endif %}</li>{% endfor %}{% endif %}</ol>
